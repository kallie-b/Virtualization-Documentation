{
  "build": {
    "content": [
      {
        "files": ["**/**.md", "**/**.yml"],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
        {
          "files": ["**/media/**"],
          "exclude": ["**/obj/**"]
        }
    ],
    "externalReference": [],
    "template": "op.html",
    "dest": "virtualization/community",
    "globalMetadata": {
      "breadcrumb_path": "/virtualization/toc.json"
    }
  }
}